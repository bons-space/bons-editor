    "standard-version": "^9.5.0",
    "terser": "^5.15.0",
    "typescript": "^4.8.3",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.1.3",
    "vite-svg-loader": "^3.6.0",
    "vue": "^3.2.38",
    "vue-eslint-parser": "^9.1.0",
    "vue-router": "^4.1.3",
    "vue-tsc": "^0.40.13",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged --no-verify",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "engines": {
    "node": ">= 14"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "yarn lint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
