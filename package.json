    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "element-plus": "^2.2.17",
    "esbuild": "^0.15.11",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
